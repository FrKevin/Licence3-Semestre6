dsFor: 'player-human' stamp: 'LauraRisani 5/27/2015 16:12'!handleMenuInputFor: aBGHuman	aBGHuman handleMenu! !!BGKeyStroke methodsFor: 'accessing' stamp: 'LauraRisani 5/27/2015 17:11'!keyString	^ self input keyString! !!BGUserInput class methodsFor: 'as yet unclassified' stamp: 'LauraRisani 5/27/2015 17:11'!from: userInput	^ self new input: userInput! !!BGUserInput methodsFor: 'accessing' stamp: 'LauraRisani 5/27/2015 17:11'!input	^ input! !!BGUserInput methodsFor: 'accessing' stamp: 'LauraRisani 5/27/2015 17:11'!input: anObject	input := anObject! !!BGBoardSpace commentStamp: 'LauraRisani 4/7/2015 18:35' prior: 33939686!I'm the knowledge of a rectangular board space, composed by many BoardPlace.Know and access space places (BGBoardPlace)!!BGBoardSpace class methodsFor: 'bases' stamp: 'LauraRisani 3/14/2015 14:52'!all: aBGBoardPlaceClass framedOfWidth: widthInteger height: heightInteger	| new |	new := self		rows: heightInteger		columns: widthInteger		tabulate: [ :i :j | aBGBoardPlaceClass new ] "up border".	new		atRows: 1		to: 1		columns: 1		to: widthInteger		putInstanceOf: BGUpBorder "right border".	new		atRows: 1		to: heightInteger		columns: widthInteger		to: widthInteger		putInstanceOf: BGRightBorder "down border".	new		atRows: heightInteger		to: heightInteger		columns: 1		to: widthInteger		putInstanceOf: BGDownBorder "left border".	new		atRows: 1		to: heightInteger		columns: 1		to: 1		putInstanceOf: BGRightBorder "corners".	new		at: 1			at: 1			put: BGUpLeftCorner new;		at: 1			at: widthInteger			put: BGUpRightCorner new;		at: heightInteger			at: widthInteger			put: BGDownRightCorner new;		at: heightInteger			at: 1			put: BGDownLeftCorner new.	^ new! !!BGBoardSpace class methodsFor: 'bases' stamp: 'LauraRisani 5/19/2015 14:45'!frameWidth: widthInteger height: heightInteger	| new |	new := self		rows: heightInteger		columns: widthInteger "up border".	new		atRows: 1		to: 1		columns: 1		to: widthInteger		putInstanceOf: BGUpBorder "right border".	new		atRows: 1		to: heightInteger		columns: widthInteger		to: widthInteger		putInstanceOf: BGRightBorder "down border".	new		atRows: heightInteger		to: heightInteger		columns: 1		to: widthInteger		putInstanceOf: BGDownBorder "left border".	new		atRows: 1		to: heightInteger		columns: 1		to: 1		putInstanceOf: BGRightBorder "corners".	new		at: 1			at: 1			put: BGUpLeftCorner new;		at: 1			at: widthInteger			put: BGUpRightCorner new;		at: heightInteger			at: widthInteger			put: BGDownRightCorner new;		at: heightInteger			at: 1			put: BGDownLeftCorner new.	^ new! !!BGBoardSpace class methodsFor: 'levels' stamp: 'LauraRisani 3/12/2015 21:14'!level1	self shouldBeImplemented! !!BGBoardSpace class methodsFor: 'levels' stamp: 'LauraRisani 3/15/2015 15:29'!level: anInteger	^ self perform: ( 'level' , anInteger asString ) asSymbol! !!BGBoardSpace methodsFor: 'accessing' stamp: 'LauraRisani 4/7/2015 16:23'!atCoordinates: pointsCollection putInstanceOf: aBGBoardPlaceClass	pointsCollection		do:			[ :point | 			self				at: point x				at: point y				put: aBGBoardPlaceClass new ]! !!BGBoardSpace methodsFor: 'accessing' stamp: 'LauraRisani 4/7/2015 16:23'!atRows: r1 to: r2 columns: c1 to: c2 putCopy: aBGBoardPlace	| m |	m := Matrix		rows: r2 - r1 + 1		columns: c2 - c1 + 1		tabulate: [ :i :j | aBGBoardPlace deepCopy ].	self		atRows: r1		to: r2		columns: c1		to: c2		put: m! !!BGBoardSpace methodsFor: 'accessing' stamp: 'LauraRisani 4/7/2015 16:23'!atRows: r1 to: r2 columns: c1 to: c2 putInstanceOf: aBGBoardPlaceClass	| m |	m := Matrix		rows: r2 - r1 + 1		columns: c2 - c1 + 1		tabulate: [ :i :j | aBGBoardPlaceClass new ].	self		atRows: r1		to: r2		columns: c1		to: c2		put: m! !"MetaBoard"!!BPHuman commentStamp: 'LauraRisani 3/26/2015 23:05' prior: 0!human!!BPLevelChanger commentStamp: 'LauraRisani 3/26/2015 23:06' prior: 0!action of deciding if and changin