e methodsFor: 'accessing' stamp: 'LauraRisani 3/17/2015 00:01'!putDownBorderAtRows: r1 to: r2 columns: c1 to: c2	self		atRows: r1		to: r2		columns: c1		to: c2		putComponent: BGDownBorder! !!EBoardSpace methodsFor: 'accessing' stamp: 'LauraRisani 3/17/2015 00:01'!putLeftBorderAtRows: r1 to: r2 columns: c1 to: c2	self		atRows: r1		to: r2		columns: c1		to: c2		putComponent: BGLeftBorder! !!EBoardSpace methodsFor: 'accessing' stamp: 'LauraRisani 3/17/2015 00:01'!putRightBorderAtRows: r1 to: r2 columns: c1 to: c2	self		atRows: r1		to: r2		columns: c1		to: c2		putComponent: BGRightBorder! !!EBoardSpace methodsFor: 'accessing' stamp: 'LauraRisani 3/17/2015 00:01'!putUpBorderAtRows: r1 to: r2 columns: c1 to: c2	self		atRows: r1		to: r2		columns: c1		to: c2		putComponent: BGUpBorder! !!EBoardSpace methodsFor: 'accessing' stamp: 'LauraRisani 3/17/2015 00:01'!putWallsAtRows: r1 to: r2 columns: c1 to: c2	self		atRows: r1		to: r2		columns: c1		to: c2		putComponent: BGWall! !!EBoardSpace methodsFor: 'coordinates' stamp: 'LauraRisani 3/16/2015 23:57'!scoreItemCoordinates	^ 24 @ 15! !!EBoardSpace methodsFor: 'positions' stamp: 'LauraRisani 3/19/2015 13:53'!scoreItemPositionOn: aEBoard	^ BGPosition		board: aEBoard		row: self scoreItemCoordinates x		column: self scoreItemCoordinates y! !!EBoardSpace methodsFor: 'query' stamp: 'LauraRisani 3/17/2015 00:00'!shouldBeTriFloorAt: row at: column	"for a half layout"	( self		willBeInnerAt: row		at: column )		ifFalse: [ ^ false ].	column = self columnCount		ifTrue: [ ^ false ].	( self		at: row		at: column ) class = EBiFloor		ifFalse: [ ^ false ].	^ 2		<			( ( self				adyacentsAt: row				at: column ) count: [ :each | each class = EBiFloor ] )! !!EBoardSpace methodsFor: 'query' stamp: 'LauraRisani 3/16/2015 23:57'!willBeInnerAt: i at: j"for a half layout if it will be inner in its full layout"	^self class		willBeInnerAt: i		at: j! !!EData commentStamp: 'LauraRisani 3/26/20