rs.	self players		at: 'eaterMover'			put: ( EEaterMover director: self );		at: 'monster1Mover'			put:				( EMonsterMover					director: self					monster: 'monster1'					attackTargetBlock: self monster1AttackPositionBlock					next: 'monster2Mover' );		at: 'monster2Mover'			put:				( EMonsterMover					director: self					monster: 'monster2'					attackTargetBlock: self monster2AttackPositionBlock					next: 'monster3Mover' );		at: 'monster3Mover'			put:				( EMonsterMover					director: self					monster: 'monster3'					attackTargetBlock: self monster3AttackPositionBlock					next: 'monster4Mover' );		at: 'monster4Mover'			put:				( EMonsterMover					director: self					monster: 'monster4'					attackTargetBlock: self monster4AttackPositionBlock					next: 'scoreItemPlacer' );		at: 'scoreItemPlacer'			put: ( EScoreItemPlacer director: self )! !!PacManGame methodsFor: 'initialization' stamp: 'LauraRisani 3/26/2015 20:52'!monster1AttackPositionBlock	^ [ :x | x eater position ]! !!PacManGame methodsFor: 'initialization' stamp: 'LauraRisani 3/26/2015 20:53'!monster2AttackPositionBlock	^ [ :x | 	x eater position		at: BGUp new		times: 4 ]! !!PacManGame methodsFor: 'initialization' stamp: 'LauraRisani 3/26/2015 20:53'!monster3AttackPositionBlock	^ [ :x | 	x eater position		at: BGDown new		times: 4 ]! !!PacManGame methodsFor: 'initialization' stamp: 'LauraRisani 3/26/2015 20:51'!monster4AttackPositionBlock	^ [ :x | 	( x monster position distanceTo: x eater position ) > 7		ifTrue: [ x eater position ]		ifFalse: [ x disperseTargetPosition ] ]! !!PacManGame methodsFor: 'players' stamp: 'LauraRisani 3/18/2015 23:57'!monsterMoverOf: aEMonster	^ self monsterMoverPlayers detect: [ :x | x monster = aEMonster ]! !!PacManGame methodsFor: 'players' stamp: 'LauraRisani 3/18/2015 15:49'!monsterMoverPlayers	^ OrderedCollection new		add: ( self player: 'monster1Mover' );		add: ( self player: 'monster2Mover' );		add: ( self player: 'monste