5 18:01'!extendedVerticallyMirrored	| answer |	answer := self class		rows: self rowCount		columns: self columnCount * 2.	answer		atRows: 1		to: answer rowCount		columns: 1		to: answer columnCount / 2		put: self.	answer		atRows: 1		to: answer rowCount		columns: answer columnCount / 2 + 1		to: answer columnCount		put: self deepCopy mirrorColumns.	^ answer! !!EBoardSpace methodsFor: 'accessing' stamp: 'LauraRisani 3/17/2015 00:08'!fillWithBigBullets	self bigBulletCoordinates		do:			[ :each | 			( self				at: each x				at: each y ) content: EBigBullet new ]! !!EBoardSpace methodsFor: 'accessing' stamp: 'LauraRisani 3/19/2015 13:20'!fillWithBullets	self		fillWithBigBullets;		fillWithSmallBullets;		bulletsNumber: ( self floors count: [ :each | each content isKindOf: EBullet ] )! !!EBoardSpace methodsFor: 'accessing' stamp: 'LauraRisani 3/18/2015 14:04'!fillWithSmallBullets	| fillCoordinates |	fillCoordinates := self floorCoordinates difference: self notSmallBulletCoordinates.	fillCoordinates		do:			[ :each | 			( self				at: each x				at: each y ) content: ESmallBullet new ]! !!EBoardSpace methodsFor: 'coordinates' stamp: 'LauraRisani 3/18/2015 14:03'!floorCoordinates	^ self		withIndicesInject: OrderedCollection new		into:			[ :partial :each :i :j | 			( each isKindOf: BGFloor )				ifTrue:					[ 					partial						add: i @ j;						yourself ]				ifFalse: [ partial ] ]! !!EBoardSpace methodsFor: 'accessing' stamp: 'LauraRisani 3/17/2015 00:02'!floors	^ self asOrderedCollection select: [ :each | each isKindOf: BGFloor ]! !!EBoardSpace methodsFor: 'positions' stamp: 'LauraRisani 3/19/2015 13:53'!initialPositionForMonster: aNumber on: anEGBoard	| coordinates |	coordinates := self monsterInitialCoordinates at: aNumber.	^ BGPosition		board: anEGBoard		row: coordinates x		column: coordinates y! !!EBoardSpace methodsFor: 'positions' stamp: 'LauraRisani 3/19/2015 13:53'!initialPositionOn: aEBoard for: movableString